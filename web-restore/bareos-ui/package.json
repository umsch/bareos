{
  "name": "bareos-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "grpc-gen": "rm ./src/generated/* && protoc --ts_out ./src/generated/ --proto_path ../../core/src/dird/connection_plugin/grpc/ restore.proto database.proto config.proto --ts_opt generate_dependencies",
    "grpc-server-up": "podman-compose -f ../../compose.yaml up -d",
    "grpc-server-down": "podman-compose -f ../../compose.yaml down"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@oruga-ui/oruga-next": "^0.8.12",
    "@oruga-ui/theme-bulma": "^0.3.0",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "grpc-web": "^1.5.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "google-protobuf": "^3.21.4",
    "install": "^0.13.0",
    "jsdom": "^24.1.0",
    "npm": "^10.8.2",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "protoc-gen-grpc-web": "^1.5.0",
    "protoc-gen-js": "^3.21.2",
    "sass-embedded": "^1.77.8",
    "start-server-and-test": "^2.0.4",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
