{
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^vue$",
    "^pinia$",
    "^@?quasar$",
    "^boot(.*)$",
    "^app(.*)$",
    "^assets(.*)$",
    "^layouts(.*)$",
    "^pages(.*)$",
    "^stores(.*)$",
    "^components(.*)$",
    "^src(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
