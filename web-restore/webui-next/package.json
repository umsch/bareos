{
  "name": "bareos-ui",
  "version": "0.0.1",
  "description": "Bareos - one page client",
  "productName": "Bareos Web Ui",
  "author": "Bareos GmbH & Co. KG",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\"",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "grpc-gen": "protoc --ts_out ./src/generated/ --proto_path ../../core/src/dird/connection_plugin/grpc/ restore.proto database.proto config.proto --ts_opt generate_dependencies --ts_opt long_type_number",
    "grpc-server-up": "podman-compose -f ../../compose.yaml up -d",
    "grpc-server-down": "podman-compose -f ../../compose.yaml down"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^4.0.1",
    "quasar": "^2.16.11",
    "vue": "^3.4.18",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "@quasar/app-vite": "^1.9.0",
    "@quasar/cli": "^2.4.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "grpc-web": "^1.5.0",
    "json-bigint-patch": "^0.0.8",
    "prettier": "^2.5.1",
    "typescript": "~5.5.4",
    "vite-plugin-checker": "^0.7.2",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
